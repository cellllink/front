{
  "name": "antd-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    ">>>> anywhere <<<<": "",
    "anywhere::website": "anywhere -d ./dist/website -p 5000",
    "anywhere::oauth": "anywhere -d ./dist/oauth -p 5010",
    ">>>> dev <<<<": "",
    "dev::website": "vite --port=5000 --host -- --project=website",
    "dev::oauth  ": "vite --port=5010 --host -- --project=oauth",
    "dev::space  ": "vite --port=5020 --host -- --project=space",
    "dev::manage ": "vite --port=5030 --host -- --project=manage",
    ">>>> build <<<<": "",
    "prod::website": "tsc && vite build -- --project=website",
    "prod::oauth  ": "tsc && vite build -- --project=oauth",
    "prod::space  ": "tsc && vite build -- --project=space",
    "prod::manage ": "tsc && vite build -- --project=manage",
    "-------------": "",
    "braft-editor": "^2.3.9"
  },
  "dependencies": {
    "@loadable/component": "^5.16.4",
    "ahooks": "^3.8.0",
    "antd": "^5.22.2",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "js-cookie": "^3.0.5",
    "primeicons": "^7.0.0",
    "primereact": "^10.6.6",
    "query-string": "^9.0.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "rxjs": "^7.8.1",
    "socket.io-client": "^4.7.5",
    "swr": "^2.2.5",
    "uuid": "^10.0.0",
    "zustand": "^4.5.2"
